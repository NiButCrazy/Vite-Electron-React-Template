{
  "name": "vite-electron-react-ts-template",
  "version": "0.0.0",
  "description": "a electron app",
  "type": "module",
  "main": ".out/main/index.js",
  "author": "Ni But Crazy",
  "license": "MIT",
  "scripts": {
    "start": "chcp 65001 && electron-vite preview ",
    "dev": "chcp 65001 && electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:app": "tsc --noEmit -p tsconfig.app.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:app",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "version": "nbc-auto-changelog -p && git add CHANGELOG.md"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.4",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.2",
    "electron-builder-squirrel-windows": "26.0.12",
    "electron-vite": "^4.0.1",
    "less": "^4.4.2",
    "nbc-auto-changelog": "^1.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "typescript": "^5.9.3",
    "vite": "^6.3.6",
    "zustand": "^5.0.8"
  }
}
